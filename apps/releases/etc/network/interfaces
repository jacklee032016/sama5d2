# /etc/network/interfaces by muxCtrl
# this file should be recreated at the time all configuration is generated when it runs first time
# 2019-08-08 15:24:05

# The loopback interface
auto lo
iface lo inet loopback

# wire interface eth0
auto eth0
iface eth0 inet dhcp
        hwaddress ether fc:c2:3d:0d:16:11

